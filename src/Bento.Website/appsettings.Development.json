{
	"$schema": "./appsettings-schema.json",
	"Serilog": {
		"MinimumLevel": {
			"Default": "Information"
		},
		"WriteTo": [
			{
				"Name": "Async",
				"Args": {
					"configure": [
						{
							"Name": "Console"
						}
					]
				}
			}
		]
	},
	"Umbraco": {
		"CMS": {
			"Content": {
				"MacroErrors": "Throw"
			},
			"Global": {
				"Smtp": {
					"From": "your@email.here",
					"Host": "localhost",
					"Port": 25
				}
			},
			"Hosting": {
				"Debug": true
			},
			"RuntimeMinification": {
				"UseInMemoryCache": false,
				"CacheBuster": "Version",
				"Version": "1"
			}
		}
	}
}